<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.credit.mapper.DailyInterestAmountRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.credit.pojo.DailyInterestAmountRecord">
        <id column="record_date" property="recordDate" />
        <id column="prc_id" property="prcId" />
        <result column="interest_amount" property="interestAmount" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        interest_amount, record_date, prc_id
    </sql>

</mapper>
